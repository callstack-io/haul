    "@types/is-ci": "^2.0.0",
    "@types/jest": "^24.0.17",
    "@types/loader-utils": "^1.1.3",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/mkdirp": "^0.5.2",
