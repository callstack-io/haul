exports[`display command help (start) 1`] = `
"haul start [options]
Starts a new webpack server

Options:

  --port [number]                  Port to run your webpack server, 8081 by default
  --dev [true|false]               Whether to build in development mode, true by default
  --no-interactive [true|false]    Disables prompting the user if the port is already in use
  --minify [true|false]            Whether to minify the bundle, 'true' by default when dev=false
  --assets-dest [string]           Path to directory where to store generated assets, eg. /tmp/dist
  --config [string]                Path to config file, eg. haul.config.js, haul.config.js by
                                   default
  --eager [parse]                  Disable lazy building for platforms (list is separated by ','),
                                   'false' by default
  --hot-reloading [true|false]     Enables hot reloading, true by default"
`;

exports[`display help 1`] = `
"
Usage: haul [command] [options]

Options:

  --version  print the version number
  --help     print usage information

Commands:

  init       Generates necessary configuration files
  start      Starts a new webpack server
  bundle     Builds the app bundle for packaging. Run with \`--platform\` flag to
             specify the platform [ios|android].
  reload     Sends reload request to all devices that enabled live reload

Run haul COMMAND --help for more information on specific commands
"
`;

